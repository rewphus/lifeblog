				{{> custom/config}}

        function MG(config) {

            this.gaUa = null;
            this.disqusShortname = null;

            var self = this;

            if ( typeof config.gaUa !== undefined && config.gaUa !== 'UA-XXXXX-X' ) {
                self.gaUa = config.gaUa;
            }
            else {
                console.info('Google Analytics Key is not set or invalid!');
            }

            if ( typeof config.disqusShortname !== undefined && config.disqusShortname !== 'example' ) {
                self.disqusShortname = config.disqusShortname;
            }
            else {
                console.info('Disqus is not activated!');
            }

        }

        MG.prototype.isAnalyticsActive = function () {
            return this.gaUa !== null;
        };

        MG.prototype.isDisqusActive = function () {
            return this.disqusShortname !== null;
        };

        MG.prototype.getGAKey = function () {
            return this.gaUa;
        };

        MG.prototype.getDisqusShortname = function () {
            return this.disqusShortname;
        };

        var mg = new MG(config);
    </script>
		{{ ghost_head }}
</head>
